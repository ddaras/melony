{
  "name": "@melony/runtime",
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepack": "npm run build",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "ai",
    "runtime",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "description": "Runtime for Melony",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^24.10.1",
    "rimraf": "^5.0.10",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@melony/core": "^0.0.1",
    "zod": "^4.1.11"
  }
}
