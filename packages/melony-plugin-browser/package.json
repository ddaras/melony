{
  "name": "@melony/plugin-browser",
  "version": "0.1.5",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.js",
      "default": "./dist/ui.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "melony": "workspace:*",
    "zod": "^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.13",
    "@melony/ui-kit": "workspace:*",
    "ai": "^6.0.42",
    "playwright": "^1.45.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "melony": "workspace:*",
    "rimraf": "^5.0.10",
    "tsup": "^8.3.0",
    "typescript": "^5.5.4",
    "zod": "^4.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
