{
  "name": "@melony/plugin-ai-sdk",
  "version": "0.1.3",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "prepack": "node ../../scripts/replace-workspace-protocols.mjs && npm run build",
    "postpack": "node ../../scripts/restore-workspace-protocols.mjs"
  },
  "dependencies": {
    "ai": "^6.0.42"
  },
  "peerDependencies": {
    "melony": "workspace:*",
    "zod": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "melony": "workspace:*",
    "zod": "^4.3.5",
    "tsup": "^8.3.0",
    "typescript": "^5.5.4",
    "rimraf": "^5.0.10",
    "@types/node": "^20.14.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
