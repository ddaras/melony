    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "melony": "workspace:*"
  },
  "devDependencies": {
    "melony": "workspace:*",
    "tsup": "^8.3.0",
    "typescript": "^5.5.4",
    "rimraf": "^5.0.10",
    "@types/node": "^20.14.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
