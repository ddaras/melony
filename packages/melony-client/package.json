{
  "name": "@melony/client",
  "version": "0.0.2",
  "main": "dist/index.js",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepack": "npm run build",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "ai",
    "client",
    "melony"
  ],
  "author": "",
  "license": "MIT",
  "description": "Framework-agnostic client for Melony",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@melony/core": "workspace:*",
    "node-html-parser": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "rimraf": "^5.0.10",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}

